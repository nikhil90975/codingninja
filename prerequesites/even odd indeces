#include<bits/stdc++.h>
using namespace std;
#define MAX 1000000
int main()
{
    int A[MAX],n,even=0,odd=0;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>A[i];
        if(i%2==0 && A[i]%2==0){
            even+=A[i];
        }
        else if(i%2!=0 && A[i]%2!=0){
            odd+=A[i];
        }
    }
    cout<<even<<" "<<odd;
	return 0;
}
